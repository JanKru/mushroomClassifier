{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
      "semi": "error",
      "no-console": "off",
      "max-len": ["error", { "ignoreStrings": true }],
      "linebreak-style": ["error", "windows"]
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true
  }
}
